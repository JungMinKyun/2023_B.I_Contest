 :\n","        time_list=[]\n","        for p in range( idx, len(data)-1) :\n","            if (data[Y_valuse][p+1]-data[Y_valuse][p]).days > 1 :\n","                for t in range(int((data[Y_valuse][p+1]-data[Y_valuse][p]).days)-1):\n","                    time_list.append(data[Y_valuse][p]+datetime.timedelta( days = (t+1) ))\n","\n","                break\n","\n","        length=len(time_list)\n","        input_date=time_list\n","        input_value=[np.nan for x in range(len(time_list))]\n","        new_data=pd.DataFrame({ Y_valuse : input_date,\n","                X_values : input_value\n","                })\n","\n","        idx = p+1\n","        temp1 = data[data.index < idx]\n","        temp2 = data[data.index >= idx]\n","        data = temp1.append(new_data,ignore_index=True).append(temp2, ignore_index=True)\n","        idx=idx\n","\n","        if length==0:\n","            break\n","\n","    return data"],"metadata":{"id":"rmfX5fffr2gv"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["data = Processing_of_MissingValues(data, '날짜', '종가')"],"metadata":{"id":"1U_NpFR3swnN"},"execution_count":null,"outputs":[]},{"cell_type":"markdown","source":["2012-01-01 추가"],"metadata":{"id":"rwV3j_reJk6a"}},{"cell_type":"code","source":["str_datetime = '2012-01-01'\n","date = datetime.datetime.strptime( str_datetime, '%Y-%m-%d' )\n","new_data = pd.DataFrame( data = { '날짜' : date, '종가' : 1155.15 }, index = [0] )"],"metadata":{"id":"rFt3Zt6LxTgN"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["data = pd.concat( [new_data, data] )"],"metadata":{"id":"1pC44cKbxqzA"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["data.reset_index( drop = True, inplace = True )"],"metadata":{"id":"WDMYWTM8Jo5F"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["data"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":424},"id":"ivl3Ol_Yp21s","executionInfo":{"status":"ok","timestamp":1687278575800,"u